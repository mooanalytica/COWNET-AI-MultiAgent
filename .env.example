# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# LangChain API Key (for tracing)
LANGSMITH_API_KEY=your_langchain_api_key

LANGSMITH_PROJECT=your_langchain_project_name

LANGSMITH_TRACING=true

# Working Directory
WORKING_DIRECTORY=./data

# Conda Environment
CONDA_ENV=base

# ChromeDriver Path
CHROMEDRIVER_PATH=./chromedriver/chromedriver

# PostgreSQL/pgvector Configuration for mem0 Long-Term Memory and Checkpointer
# Used by both mem0 memory layer and LangGraph async checkpointer
PGVECTOR_HOST=localhost
PGVECTOR_PORT=5432
PGVECTOR_USER=postgres
PGVECTOR_PASSWORD=your_postgres_password
PGVECTOR_DATABASE=cownet_memory
